generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model tops {
  id                  Int     @id @default(autoincrement())
  itemName            String  @unique
  maxtemp_resistant   Int
  mintemp_resistant   Int
  wind_resistant      Int
  rain_resistant      Int
  snow_resistant      Int
  heat_resistant      Int
  uv_resistant        Int
}

